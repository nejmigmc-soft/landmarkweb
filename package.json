{
  "name": "landmark-project",
  "private": true,
  "packageManager": "pnpm@9",
  "scripts": {
    "dev": "concurrently -k -n API,WEB -c cyan,green \"pnpm --filter @landmark/api dev\" \"pnpm --filter @landmark/web dev --port 3002\"",
    "prisma:generate": "prisma generate --schema packages/db/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema packages/db/prisma/schema.prisma --name init",
    "db:seed": "ts-node --transpile-only -P packages/db/tsconfig.json packages/db/prisma/seed.ts",
    "build": "pnpm -r run build",
    "start": "pnpm --filter @landmark/api start & pnpm --filter @landmark/web start"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.19.11",
    "concurrently": "^9.0.0",
    "prisma": "6.14.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@prisma/client": "6.14.0",
    "bcryptjs": "^2.4.3",
    "lucide-react": "^0.441.0"
  },
  "overrides": {
    "prisma": "6.14.0",
    "@prisma/client": "6.14.0"
  }
}
